{
    "presets": [
        ["env",
          {
            "targets": {
              "browsers": [
                "Chrome >= 60",
                "FireFox >= 57",
                "Safari >= 10",
                "Explorer 11",
                "last 4 Edge versions"
              ]
            },
            "modules": false,
            "useBuiltIns": true
          }
      ]
    ],
    "ignore": [
      "node_modules"
    ],
    "plugins": [
        ["transform-runtime", {
        "helpers": true,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
        }]
    ]
  }
  